# Your snippets
#
# Atom snippets allow you to enter a simple prefix in the editor and hit tab to
# expand the prefix into a larger code block with templated values.
#
# You can create a new snippet in this file by typing "snip" and then hitting
# tab.
#
# An example CoffeeScript snippet to expand log to console.log:
#
# '.source.coffee':
#   'Console log':
#	 'prefix': 'log'
#	 'body': 'console.log $1'
#
# Each scope (e.g. '.source.coffee' above) can only be declared once.
#
# This file uses CoffeeScript Object Notation (CSON).
# If you are unfamiliar with CSON, you can read more about it in the
# Atom Flight Manual:
# http://flight-manual.atom.io/using-atom/sections/basic-customization/#_cson

'.text.tex.latex':

	'Subfile init':
		'prefix': 'subfile'
		'body': '''
		\\\\providecommand{\\\\main}{../../main}
		\\\\providecommand{\\\\figpath}[1]{\\\\main/../lessons/\#1}
		\\\\documentclass[${1:../main/main.tex}]{subfiles}



		\\\\begin{document}
		$2
		\\\\end{document}
		'''

	'Date':
		'prefix': 'date'
		'body': '''
		\\\\newdate{date}{$1}{$2}{2019}
		'''

	'Subfile Lecture':
		'prefix': 'subfile lecture'
		'body': '''
		\\\\documentclass[../main/main.tex]{subfiles}

		\\\\newdate{date}{$1}{$2}{2019}

		\\\\begin{document}

		\\\\marginpar{ \\\\textbf{Lecture $3} \\\\\\\\\\  \\\\displaydate{date}. \\\\\\\\\\ Compiled:  \\\\today.}
		$4
		\\\\end{document}
		'''

	'SVG figure':
		'prefix': 'svgfig'
		'body': '''
		\\\\begin{figure}[!h]
			\\\\centering
			\\\\incfig{figures/$1}
			\\\\caption{$2}
			\\\\label{fig:$1}
		\\\\end{figure}
		'''

	'Regular citation':
		'prefix': 'cite'
		'body': '\\\\cite{$2}'

	'Cursive':
		'prefix': 'em'
		'body': '\\\\emph{$1} $2'

	'Python Code':
		'prefix': 'code'
		'body': '''
		\\\\begin{figure}[!h]
			\\\\begin{lstlisting}[language=Python]
			$1
			\\\\end{lstlisting}
			\\\\caption{\\\\label{}}
		\\\\end{figure}
		'''

	'Theorem':
		'prefix': 'theorem'
		'body': '''
		\\\\begin{theorem}[]
			$1
		\\\\end{theorem}
		'''

	'Corollary':
		'prefix': 'corollary'
		'body': '''
		\\\\begin{corollary}[]
			$1
		\\\\end{corollary}
		'''

	'Lemma':
		'prefix': 'lemma'
		'body': '''
		\\\\begin{lemma}[]
			$1
		\\\\end{lemma}
		'''

	'Definition':
		'prefix': 'definition'
		'body': '''
		\\\\begin{definition}
			$1
		\\\\end{definition}
		'''

	'Exercise':
		'prefix': 'exercise'
		'body': '''
		\\\\begin{exercise}
			$1
		\\\\end{exercise}
		'''

	'Remark':
		'prefix': 'remark'
		'body': '''
		\\\\begin{remark}
			$1
		\\\\end{remark}
		'''

	'Proof':
		'prefix': 'proof'
		'body': '''
		\\\\begin{proof}
			$1
		\\\\end{proof}
		'''

	'Label':
		'prefix': 'label'
		'body': '\\\\label{$1} $2'

	'Boldface':
		'prefix': 'bold'
		'body': '\\\\textbf{$1} $2'

	'Weight':
		'prefix': 'weight'
		'body': '\\\\( \\\\mathbf{w}$1 \\\\) $2'

	'Margin text':
		'prefix': 'margin'
		'body': '\\\\marginpar{$1} $2'

	'Subequations numbered with letters':
		'prefix': 'subequations'
		'body': '''
		\\\\begin{subequations}
			\\\\begin{align}
				${1:$2 &= $3 }
			\\\\end{align}
			\\\\label{}
		\\\\end{subequations}
		'''
		'description': ''

	'Equations numbered with the same number':
		'prefix': 'equations'
		'body': '''
		\\\\begin{equation}
			\\\\begin{gathered}
				$1
			\\\\end{gathered}
			\\\\label{eq:$3}
		\\\\end{equation}
		'''
		'description': ''

	'Equations with cases':
		'prefix': 'cases eq'
		'body': '''
		\\\\begin{equation}
			$1
			\\\\begin{cases}
				$2 \\\\\\\\\\
				$3
			\\\\end{cases}
			\\\\label{eq:}
		\\\\end{equation}
		'''
		'description': ''

	'Numbered equation':
		'prefix': 'equation'
		'body': '''
		\\\\begin{equation}
			$1
			\\\\label{eq:}
		\\\\end{equation}
		'''

	'Unumbered equation':
		'prefix': 'equation not numbered'
		'body': '''
		\\\\begin{equation*}
			$1
		\\\\end{equation*}
		'''

	'Reference':
		'prefix': 'ref'
		'body': '\\\\ref{$1} $2'


	'Chemical formula':
		'prefix': 'ce'
		'body': '\\\\ce{$1} $2'
		'description': 'Needs the package mhchem'

	'Subfigure':
		'prefix': 'subfigure'
		'body': '''
		\\\\begin{subfigure}
			\\\\centering
			\\\\includegraphics[width=1\\\\textwidth]{\\\\figpath{$1}/$1_images/$2}
			\\\\caption{\\\\label{fig:$3} Description.}
		\\\\end{subfigure}%
		'''
		'description': 'Needs the package subcaption'

	'TwoFigure Subcaption':
		'prefix': 'twofiguresub'
		'body': '''
		\\\\begin{figure}[!h]
			\\\\begin{minipage}[c]{0.5\\\\linewidth}
				\\\\subfloat[][Description]{ \\\\includegraphics[width=0.8\\\\textwidth]{\\\\figpath{$1}/$1_images/$2}
				\\\\label{fig:$3} }
			\\\\end{minipage}
			\\\\begin{minipage}[]{0.5\\\\linewidth}
				\\\\centering
				\\\\subfloat[][Description]{\\\\includegraphics[width=0.8\\\\textwidth]{\\\\figpath{$4}/$4_images/$5}
				\\\\label{fig:$6} }
			\\\\end{minipage}
			\\\\caption{\\\\label{fig:} Description}
		\\\\end{figure}
		'''
		'description': ''

	'Figure':
		'prefix': 'figure'
		'body': '''
		\\\\begin{figure}[!h]
			\\\\centering
			\\\\includegraphics[width=1\\\\textwidth]{\\\\figpath{$1}/$1_images/$2}
			\\\\caption{\\\\label{fig:} Description.}
		\\\\end{figure}
		'''
		'description': ''

	'TwoFigure':
		'prefix': 'twofigure'
		'body': '''
		\\\\begin{figure}[!h]
			\\\\begin{minipage}[c]{0.5\\\\linewidth}
				\\\\centering
				\\\\includegraphics[width=1\\\\textwidth]{\\\\figpath{$1}/$1_images/$2}
			\\\\end{minipage}
			\\\\begin{minipage}[]{0.5\\\\linewidth}
				\\\\centering
				\\\\includegraphics[width=1\\\\textwidth]{\\\\figpath{$3}/$3_images/$4}
			\\\\end{minipage}
			\\\\caption{\\\\label{fig:$5} Description.}
		\\\\end{figure}
		'''
		'description': ''

	'Tikzpicture':
		'prefix': 'tikzpicture'
		'body': '''
		\\\\begin{figure}[!h]
			\\\\centering
			\\\\begin{tikzpicture}[scale=1, transform shape]
				$1
			\\\\end{tikzpicture}
			\\\\caption{\\\\label{fig:$3} Description.}
		\\\\end{figure}
		'''
		'description': ''

	'TwoFigure Tikz':
		'prefix': 'twofiguretikz'
		'body': '''
		\\\\begin{figure}[!h]
			\\\\begin{minipage}[c]{0.5\\\\linewidth}
				\\\\centering
				\\\\begin{tikzpicture}[scale=1, transform shape]
					$1
				\\\\end{tikzpicture}
			\\\\end{minipage}
			\\\\begin{minipage}[]{0.5\\\\linewidth}
				\\\\centering
				\\\\begin{tikzpicture}[scale=1, transform shape]
					$2
				\\\\end{tikzpicture}
			\\\\end{minipage}
			\\\\caption{\\\\label{fig:$3} Description.}
		\\\\end{figure}
		'''
		'description': ''

	'Figure and text':
		'prefix': 'figuretext'
		'body': '''
		\\\\begin{minipage}[c]{0.5\\\\linewidth}
			$1
		\\\\end{minipage}
		\\\\begin{minipage}[]{0.5\\\\linewidth}
			\\\\centering
			\\\\includegraphics[width=1\\\\textwidth]{\\\\figpath{$2}/$2_images/$3}
			\\\\captionof{figure}{\\\\label{} Description.}
		\\\\end{minipage}
		'''
		'description': ''

	'SIunitx Value':
		'prefix': 'siunitx'
		'body': '\\\\SI{$1}{$2} $3'

	'SIunitx number':
		'prefix': 'number'
		'body': '\\\\num{$1} $2'

	'Reference to equation':
