	'Slide':
		'prefix': 'slide'
		'body': '''
		\\\\begin{minipage}[t]{0.45\\\\linewidth}
			\\\\vspace{0pt}
			\\\\centering
			\\\\frame{
				\\\\includegraphics[page=$1,width=1\\\\textwidth]{../../pdfs/$2.pdf}
			}
		\\\\end{minipage}
		\\\\hfill
		\\\\begin{minipage}[t]{0.52\\\\linewidth}
			\\\\vspace{0pt}
			\\\\subsubsection*{Slide $1}
			$3
		\\\\end{minipage}
		\\\\vspace{5pt}
		'''
		'description': ''

